{
  "name": "joshbritzco",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 7200 --host 0.0.0.0",
    "build": "ng build --prod && yarn run generate-sw",
    "generate-sw": "workbox injectManifest workbox-config.js",
    "test": "ng test --watch=false --progress=false --browsers=ChromeNoSandboxHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:prod": "yarn run build && cd dist && serve -s -n -p 8181",
    "deploy:ci:prod": "firebase deploy --only hosting:prod --token=$FIREBASE_DEPLOY_TOKEN",
    "deploy:ci:staging": "firebase deploy --only hosting:staging --token=$FIREBASE_DEPLOY_TOKEN"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "babylon": "^6.18.0",
    "core-js": "^2.5.4",
    "prismjs": "^1.15.0",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@types/jasmine": "~3.3.15",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.6.1",
    "codelyzer": "~5.0.1",
    "firebase-tools": "^6.3.1",
    "hint": "^5.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.3.0",
    "serve": "^11.0.2",
    "ts-node": "~8.2.0",
    "tslint": "~5.16.0",
    "typescript": "~3.4.5",
    "workbox-cli": "^4.3.1"
  }
}
